import unittest
from src.extract_data import extract_data

class test_extract_data(unittest.TestCase):

    def test_ObtainRowOne(self):
        values = [101.13, 201.55, 305.22, 402.88, 509.33, 605.77, 712.22, 808.66, 915.11, 1011.55]
        emissions = [4.77, 9.21, 14.78, 19.55, 24.66, 29.11, 34.44, 39.77, 45.11, 50.44]
        ans = extract_data('sample_data.xlsx', 1, 1)
        self.assertEqual(ans, [values, emissions])